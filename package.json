{
  "name": "gitgen",
  "version": "1.0.0",
  "description": "Predictive git. Generate specs from git history, and generate new branches for feature implementations.",
  "type": "module",
  "bin": {
    "git-gen": "./gitgen.ts"
  },
  "scripts": {
    "start": "npx tsx gitgen.ts",
    "test": "dotenv -e .env -- vitest run",
    "test:unit": "dotenv -e .env -- vitest run tests/unit",
    "test:integration": "dotenv -e .env -- vitest run tests/integration",
    "test:e2e": "dotenv -e .env -- vitest run tests/e2e",
    "test:e2e:cli": "dotenv -e .env -- vitest run tests/e2e/cli.test.ts",
    "test:e2e:web": "dotenv -e .env -- playwright test",
    "test:watch": "dotenv -e .env -- vitest",
    "test:coverage": "dotenv -e .env -- vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gitgen/gitgen.git"
  },
  "keywords": [
    "gitgen",
    "ai",
    "llm",
    "code-generation",
    "anthropic",
    "claude",
    "bedrock",
    "openrouter"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^2.2.0",
    "@ai-sdk/anthropic": "^1.2.0",
    "@ai-sdk/google": "^3.0.10",
    "@ai-sdk/openai": "^3.0.14",
    "@openrouter/ai-sdk-provider": "^0.7.0",
    "ai": "^4.3.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "dotenv-cli": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  }
}
