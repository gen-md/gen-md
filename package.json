{
  "name": "gitgen",
  "version": "1.0.0",
  "description": "Predictive git. Generate specs from git history, and generate new branches for feature implementations.",
  "type": "module",
  "bin": {
    "git-gen": "./gitgen.ts"
  },
  "scripts": {
    "start": "npx tsx gitgen.ts",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gitgen/gitgen.git"
  },
  "keywords": [
    "gitgen",
    "ai",
    "llm",
    "code-generation",
    "anthropic",
    "claude",
    "bedrock",
    "openrouter"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "ai": "^4.3.0",
    "@ai-sdk/anthropic": "^1.2.0",
    "@ai-sdk/amazon-bedrock": "^2.2.0",
    "@openrouter/ai-sdk-provider": "^0.7.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  }
}
